{
	"name": "flarum-lang/serbian-cyrillic",
	"type": "flarum-extension",
	"description": "Serbian (Cyrillic) language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/serbian-cyrillic",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"sr_Cyrl-locale",
		"serbian-cyrillic-locale",
		"serbian-cyrillic"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/serbian-cyrillic/issues",
		"source": "https://github.com/flarum-lang/serbian-cyrillic"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Serbian (Cyrillic)",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "sr-Cyrl",
			"title": "Serbian (Cyrillic)"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
